                <div class="sidebar-section">
                    <h3 class="sidebar-title">
                        <span class="icon" style="background: var(--accent-orange);">&#x1F4C8;</span>
                        VVC1 Features
                    </h3>
                    
                    <div class="legend-item" data-detail="vvc-vwap">
                        <div class="legend-color" style="background: var(--accent-orange);"></div>
                        <div class="legend-text">
                            <div class="name">VWAP + B&auml;nder</div>
                            <div class="desc">Volume Weighted Average Price</div>
                        </div>
                    </div>

                    <div class="legend-item" data-detail="vvc-squeeze">
                        <div class="legend-color" style="background: var(--accent-red);"></div>
                        <div class="legend-text">
                            <div class="name">Bollinger Squeeze</div>
                            <div class="desc">BB innerhalb KC = Squeeze</div>
                        </div>
                    </div>

                    <div class="legend-item" data-detail="vvc-macd">
                        <div class="legend-color" style="background: var(--accent-green);"></div>
                        <div class="legend-text">
                            <div class="name">MACD Signale</div>
                            <div class="desc">Crosses + Zero-Line + Histogram</div>
                        </div>
                    </div>

                    <div class="legend-item" data-detail="vvc-volume">
                        <div class="legend-color" style="background: var(--accent-cyan);"></div>
                        <div class="legend-text">
                            <div class="name">Volume + CVD</div>
                            <div class="desc">Cumulative Volume Delta</div>
                        </div>
                    </div>

                    <div class="legend-item" data-detail="vvc-session">
                        <div class="legend-color" style="background: #9E9E9E;"></div>
                        <div class="legend-text">
                            <div class="name">Session Levels</div>
                            <div class="desc">High/Low + Prev Day H/L/C</div>
                        </div>
                    </div>

